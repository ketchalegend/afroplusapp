    getActionContainerClass() {
        if (_app_GlobalFields__WEBPACK_IMPORTED_MODULE_3__["GlobalFields"].site_details.actionButtonStyle == '2')
            return 'fastBtnsContainer';
        else
            return 'fastBtnsContainerFull';
    }
    getActionLabelClass() {
