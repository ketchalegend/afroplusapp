                component: _tabs_modalAdd_modalAdd__WEBPACK_IMPORTED_MODULE_4__["ModalAddNode"],
                componentProps: { userId: 8675309 }
            });
            return yield profileModal.present();
        });
    }
    closeSidebar() {
        this.menu.close('appMenu');
    }
};
