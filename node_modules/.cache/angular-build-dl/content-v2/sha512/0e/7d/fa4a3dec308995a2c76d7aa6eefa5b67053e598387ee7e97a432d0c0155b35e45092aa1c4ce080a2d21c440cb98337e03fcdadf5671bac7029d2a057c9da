let ModalPost = class ModalPost {
    constructor(viewCtrl, inputData, socialSharing, inAppBrowser) {
        this.viewCtrl = viewCtrl;
        this.socialSharing = socialSharing;
        this.inAppBrowser = inAppBrowser;
        this.Constants = _app_Constants__WEBPACK_IMPORTED_MODULE_4__["Constants"];
        this.GlobalFields = _GlobalFields__WEBPACK_IMPORTED_MODULE_6__["GlobalFields"];
        this.post = inputData.data.post;
        console.log(this.post);
        setTimeout(() => {
            let el = this.dynamic.nativeElement;
            let arrayOfLinks = el.querySelectorAll('a');
            for (let i = 0; i < arrayOfLinks.length; i++) {
                let anchor = arrayOfLinks[i];
                anchor.onclick = (ev) => {
                    console.log("Opening link");
                    const browser = this.inAppBrowser.create(anchor.href, '_blank');
                    ev.preventDefault();
                    return false;
                };
            }
            ;
        }, 1000);
        //this.service.setTransparentBackgroundStatusBar();
    }
    closeModal() {
        this.viewCtrl.dismiss();
    }
    share() {
        this.socialSharing.share("Check this link", "", "", this.post.link).then(function () {
            console.log('Successful share');
        }).catch(function (error) {
            console.log('Error sharing:', error);
        });
    }
};
