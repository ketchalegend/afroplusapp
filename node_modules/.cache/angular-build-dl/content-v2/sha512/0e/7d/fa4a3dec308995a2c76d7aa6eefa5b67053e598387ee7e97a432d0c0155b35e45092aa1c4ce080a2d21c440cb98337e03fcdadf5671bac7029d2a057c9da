    getMyAddressFromGoogle(lat, long) {
        return this.http.get("https://maps.googleapis.com/maps/api/geocode/json?latlng=" + lat + "," + long + "&key=" + _Constants__WEBPACK_IMPORTED_MODULE_2__["Constants"].GoogleMapsKEY);
