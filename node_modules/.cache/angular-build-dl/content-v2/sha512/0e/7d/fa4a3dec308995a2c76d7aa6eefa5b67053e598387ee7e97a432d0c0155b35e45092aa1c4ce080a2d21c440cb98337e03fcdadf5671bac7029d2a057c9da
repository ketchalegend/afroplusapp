    getStartsHtml(rating) {
        let num = (parseFloat(rating)) / 2;
        let res = [];
        while (num >= 1) {
            res.push("");
            num = num - 1;
        }
        if (num < 1 && num > 0)
            res.push('-half');
        for (let i = 0; i < 5 - res.length; i++) {
            res.push('-outline');
        }
        return res;
    }
